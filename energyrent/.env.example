# Energy Rental Service 环境变量配置

# 服务端口（默认 4001）
PORT=4001

# 前端 URL（用于 CORS 配置）
FRONTEND_URL=http://localhost:5173

# ===== CatFee 配置 =====
# 从 https://catfee.io/?tab=api 获取 API Key 和 Secret
# API 文档: https://docs.catfee.io/en/getting-started/buy-energy-via-api-on-catfee/nodejs
CATFEE_API_KEY=your_catfee_api_key_here
CATFEE_API_SECRET=your_catfee_api_secret_here
# 是否启用 CatFee（默认 true，设置为 false 可禁用）
CATFEE_ENABLED=true

# ===== GasStation 配置 =====
# 从 https://gasdocs-en.gasstation.ai/product-description/product-introduction/API 获取 App ID 和 Secret
GASSTATION_APP_ID=your_gasstation_app_id_here
GASSTATION_SECRET=your_gasstation_secret_here
# 是否启用 GasStation（默认 true，设置为 false 可禁用）
GASSTATION_ENABLED=true

# ===== TronFuel 配置 =====
# 从 https://tronfuel.dev 获取 API Key 和 Secret
TRONFUEL_API_KEY=your_tronfuel_api_key_here
TRONFUEL_API_SECRET=your_tronfuel_api_secret_here
# 是否启用 TronFuel（默认 true，设置为 false 可禁用）
TRONFUEL_ENABLED=true

# ===== TronXEnergy 配置 =====
# 从 https://tronxenergy.com 获取 API Key
TRONXENERGY_API_KEY=your_tronxenergy_api_key_here
# 是否启用 TronXEnergy（默认 true，设置为 false 可禁用）
TRONXENERGY_ENABLED=true
